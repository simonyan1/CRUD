<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="style.css">
</head>
<body>

    <table>
        <tr>
            <td>ID</td>
            <td>City</td>
            <td>Update</td>
            <td>Delete</td>
        </tr>
         <% obj.forEach(function(justOne) { %>
        <tr>
            <td><%= justOne.theaterId %></td>
            <td><%= justOne.location.address.city %></td>
            <td><a class="update" href="update/id=<%= justOne._id %>">Update</a></td>
            <td><a class="delete" href="delete/id=<%= justOne._id %>">Delete</a></td>
        </tr>
    <% }); %>
    </table>
